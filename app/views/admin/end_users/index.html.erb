<div class="container">
  <div class="row">
    <div class="col-6 offset-3">
      <h5>ユーザー 一覧</h5><br>
      <table>
        <thead class="text-center">
          <tr>
            <th>ID</th>
            <th>ユーザーネーム</th>
            <th>メールアドレス</th>
          </tr>
        </thead>
        <tbody>
          <% @end_users.each do |end_user| %>
            <tr>
              <td>
                <%= link_to admin_end_user_path(end_user.id) do %>
                  <%= end_user.id %>
                <% end %>
              </td>
              <td><%= end_user.nickname %></td>
              <td><%= end_user.email %></td>
            <tr>
          <% end %>
        <tbody>
      </table>
    </div>
  </div>
</div>
